{
    "id": "CASMO",
    "varprefix": "$",
    "formulaprefix": "@",
    "delim": "{}",
    "commentline": "*",
    "output": {
        "burnup": "grep -E '^[[:space:]]*[0-9]+[[:space:]]+[0-9]+\\.[0-9]+[[:space:]]+[0-9]+\\.[0-9]+[[:space:]]+[0-9]+\\.[0-9]+[[:space:]]+[0-9]+\\.[0-9]+[[:space:]]+[0-9]+\\.[0-9]+[[:space:]]+[0-9]+\\.[0-9]+[[:space:]]+[0-9]+\\.[0-9]+' *.out | tail -1 | awk '{print $8}'",
        "k_inf": "grep '2D k-infinity:' *.out | sed -E 's/.*2D k-infinity:[[:space:]]+([0-9.]+).*/\\1/'",
        "m2": "grep -E '^[[:space:]]*[0-9]+[[:space:]]+[0-9]+\\.[0-9]+[[:space:]]+[0-9]+\\.[0-9]+[[:space:]]+[0-9]+\\.[0-9]+[[:space:]]+[0-9]+\\.[0-9]+[[:space:]]+[0-9]+\\.[0-9]+[[:space:]]+[0-9]+\\.[0-9]+[[:space:]]+[0-9]+\\.[0-9]+' *.out | tail -1 | awk '{print $11}'",
        "power_peak": "grep 'Power Distribution' *.out | sed -E 's/.*Peak: LL = ([0-9.]+).*/\\1/'",
        "pin_power": "grep -A 18 'Power Distribution' *.out | tail -17 | sed 's/\\*//g'",
        "pin_exposure": "grep -A 17 'Burnup in MWd/kg' *.out | tail -17",
        "decay_constants": "grep 'Decay Constants:' *.out | awk '{print $3, $4, $5, $6, $7, $8}'",
        "beta_eff": "grep '(Beta Eff.)' *.out | sed -E 's/.*[[:space:]]([0-9]+\\.[0-9]+)[[:space:]]+\\(Beta Eff\\.\\).*/\\1/'",
        "neutron_generation_time": "grep 'Mean Neutron Generation Time' *.out | sed -E 's/.*:[[:space:]]+([0-9.E+-]+).*/\\1/'"
    }
}
